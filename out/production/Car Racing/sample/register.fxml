<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Register">
   <children>
      <ImageView fitHeight="755.0" fitWidth="1000.0">
         <image>
            <Image url="@SpeedVerse_Login_bg.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="68.0" fitWidth="82.0" layoutX="395.0" layoutY="-92.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@reg.png" />
         </image>
      </ImageView>
      <ToggleButton fx:id="register" layoutX="14.0" layoutY="365.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="314.0" style="-fx-background-color: rgba(255, 255, 255, 0.2);" text="REGISTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </ToggleButton>
      <ToggleButton fx:id="login" layoutX="14.0" layoutY="435.0" mnemonicParsing="false" prefHeight="68.0" prefWidth="314.0" style="-fx-background-color: rgba(255, 255, 255, 0.2);" text="LOG IN" textAlignment="CENTER" textFill="#2fff00">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <font>
            <Font size="20.0" />
         </font>
      </ToggleButton>
      <TextField fx:id="username" disable="true" layoutX="320.0" layoutY="593.0" prefHeight="47.0" prefWidth="150.0" />
      <TextField fx:id="password" disable="true" layoutX="517.0" layoutY="593.0" prefHeight="47.0" prefWidth="150.0" />
      <Label layoutX="344.0" layoutY="544.0" text="Username" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="544.0" layoutY="544.0" text="Password" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Button fx:id="save" disable="true" layoutX="14.0" layoutY="573.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="187.0" text="SAVE" textAlignment="CENTER" textFill="#d90909">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="32.0" />
         </font>
      </Button>
      <Button fx:id="go" disable="true" layoutX="717.0" layoutY="591.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="187.0" text="GO!" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="quit" layoutX="883.0" layoutY="701.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="102.0" text="QUIT" textAlignment="CENTER" textFill="#110a5c">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
