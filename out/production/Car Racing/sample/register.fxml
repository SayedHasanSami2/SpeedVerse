<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Register">
   <children>
      <ImageView fitHeight="771.0" fitWidth="1000.0">
         <image>
            <Image url="@SpeedVerse_Login_bg.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="131.0" fitWidth="41.0" layoutX="113.0" layoutY="-44.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Instruction_picture.png" />
         </image>
      </ImageView>
      <ToggleButton fx:id="register" layoutX="21.0" layoutY="338.0" mnemonicParsing="false" prefHeight="71.0" prefWidth="238.0" style="-fx-background-color: rgba(255, 255, 255, 0.2);" text="REGISTER" textFill="WHITE">
         <font>
            <Font size="31.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </ToggleButton>
      <ToggleButton fx:id="login" layoutX="21.0" layoutY="409.0" mnemonicParsing="false" prefHeight="71.0" prefWidth="238.0" style="-fx-background-color: rgba(255, 255, 255, 0.2);" text="LOG IN" textAlignment="CENTER" textFill="#00ff22">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <font>
            <Font size="21.0" />
         </font>
      </ToggleButton>
      <TextField fx:id="username" disable="true" layoutX="306.0" layoutY="576.0" prefHeight="42.0" prefWidth="150.0" />
      <TextField fx:id="password" disable="true" layoutX="515.0" layoutY="576.0" prefHeight="42.0" prefWidth="150.0" />
      <Label layoutX="320.0" layoutY="532.0" text="Username" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="532.0" layoutY="532.0" text="Password" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Button fx:id="save" disable="true" layoutX="33.0" layoutY="549.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="187.0" text="SAVE" textAlignment="CENTER" textFill="#d90909">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="32.0" />
         </font>
      </Button>
      <Button fx:id="go" disable="true" layoutX="713.0" layoutY="552.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="187.0" text="GO!" textAlignment="CENTER" textFill="#cf3319">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="29.0" />
         </font>
      </Button>
      <Button fx:id="quit" layoutX="852.0" layoutY="707.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="133.0" text="QUIT" textAlignment="CENTER" textFill="#110a5c">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
